<!--home.wxml  主页-->
<!--显示出会议简介-->
  <view wx:for="{{list}}" wx:if="{{item.state==2}}" id = "{{item.mid}}"bindtap="jumpToDetail" class="part">
    <text class="typical">会议名称:{{item.name}}</text>
    <text class="typical">会议时间:{{item.time}}</text>
    <text class="typical">会议地点:{{item.place}}</text>
  </view>